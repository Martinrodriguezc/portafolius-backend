{
  "name": "portafolius-backend",
  "version": "1.0.0",
  "description": "Backend PortafolIUS",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "postinstall": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:real-coverage": "jest --testPathPatterns=\"real-coverage\" --coverage",
    "test:unit": "jest --testPathIgnorePatterns=\"integration\"",
    "test:integration": "jest --testPathPattern=\"integration\"",
    "test:middleware": "jest tests/middleware --coverage",
    "test:controllers": "jest tests/controllers --coverage",
    "coverage:open": "open coverage/index.html",
    "coverage:report": "jest --coverage && open coverage/index.html",
    "coverage:threshold": "jest --coverage --coverageThreshold='{\"global\":{\"branches\":50,\"functions\":60,\"lines\":60,\"statements\":60}}'",
    "test:debug": "jest --detectOpenHandles --forceExit --verbose",
    "test:ci": "jest --coverage --watchAll=false --ci",
    "coverage:clean": "rm -rf coverage",
    "test:specific": "jest --testNamePattern",
    "test:parallel": "jest --maxWorkers=4"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.787.0",
    "@aws-sdk/s3-request-presigner": "^3.787.0",
    "aws-sdk": "^2.1692.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "ffmpeg-static": "^5.2.0",
    "ffprobe-static": "^3.1.0",
    "fluent-ffmpeg": "^2.1.3",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.15.6",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/aws-sdk": "^0.0.42",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/ffprobe-static": "^2.0.3",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.17.47",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/pg": "^8.11.14",
    "@types/supertest": "^6.0.3",
    "jest": "^30.0.0",
    "nodemon": "^3.1.9",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
